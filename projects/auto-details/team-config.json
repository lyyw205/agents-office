  "workflows": [
    {
      "name": "reference-to-widgets",
      "description": "레퍼런스 이미지 → HTML 위젯 추출",
      "steps": [
        {
          "order": 1,
          "name": "Behance 스크래핑",
          "agent": "playwright",
          "skill": "playwright",
          "input": "Behance URL",
          "output": "references/ 폴더에 이미지 저장"
        },
        {
          "order": 2,
          "name": "이미지 초기 분석",
          "agent": "vision",
          "skill": null,
          "input": "레퍼런스 이미지",
          "output": "바운딩 박스 좌표"
        },
        {
          "order": 3,
          "name": "디자인 시스템 추출",
          "agent": "designer",
          "skill": "ui-designer",
          "input": "레퍼런스 이미지",
          "output": "preset--ref-{name}.json"
        },
        {
          "order": 4,
          "name": "위젯 HTML 생성",
          "agent": "designer",
          "skill": "web-artifacts-builder",
          "input": "디자인 시스템 + 바운딩 박스",
          "output": "*.widget.html 파일들"
        },
        {
          "order": 5,
          "name": "품질 검증",
          "agent": "code-reviewer",
          "skill": null,
          "input": "생성된 위젯 HTML",
          "output": "검증 리포트"
        },
        {
          "order": 6,
          "name": "레지스트리 업데이트",
          "agent": "executor",
          "skill": null,
          "input": "검증된 위젯",
          "output": "widgets/_registry.json 업데이트"
        }
      ],
      "estimatedTime": "15-20분",
      "parallelizable": false
    },
    {
      "name": "product-to-html",
      "description": "제품 정보 → HTML 상세페이지",
      "steps": [
        {
          "order": 1,
          "name": "섹션 플래닝",
          "agent": "architect",
          "skill": null,
          "input": "제품 정보",
          "output": "section-plan.json"
        },
        {
          "order": 2,
          "name": "위젯 선택",
          "agent": "designer",
          "skill": null,
          "input": "섹션 플랜 + 스타일 선호",
          "output": "widget-selection.json"
        },
        {
          "order": 3,
          "name": "HTML 조합",
          "agent": "executor",
          "skill": "web-artifacts-builder",
          "input": "위젯 선택 + 제품 정보",
          "output": "{product}-detail.html"
        },
        {
          "order": 4,
          "name": "스타일 품질 개선",
          "agent": "designer",
          "skill": "frontend-design",
          "input": "생성된 HTML",
          "output": "스타일 개선된 HTML"
        },
        {
          "order": 5,
          "name": "최종 검증",
          "agent": "qa-tester",
          "skill": null,
          "input": "최종 HTML",
          "output": "브라우저 테스트 결과"
        }
      ],
      "estimatedTime": "10-15분",
      "parallelizable": false
    },
    {
      "name": "quality-review",
      "description": "위젯 품질 검수",
      "steps": [
        {
          "order": 1,
          "name": "보안 검사",
          "agent": "security-reviewer",
          "skill": null,
          "input": "위젯 HTML",
          "output": "보안 리포트",
          "parallel": true
        },
        {
          "order": 1,
          "name": "성능 검사",
          "agent": "performance-reviewer",
          "skill": null,
          "input": "위젯 HTML",
          "output": "성능 리포트",
          "parallel": true
        },
        {
          "order": 1,
          "name": "코드 품질 검사",
          "agent": "code-reviewer",
          "skill": null,
          "input": "위젯 HTML",
          "output": "코드 품질 리포트",
          "parallel": true
        },
        {
          "order": 2,
          "name": "결과 통합",
          "agent": "qa-tester",
          "skill": null,
          "input": "3개 리포트",
          "output": "최종 검수 결과"
        }
      ],
      "estimatedTime": "5-8분",
      "parallelizable": true
    }
  ],

